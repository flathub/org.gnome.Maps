        },
        {
            "name": "gnome-maps",
            "buildsystem": "meson",
            "sources": [
                {
                    "type": "archive",
                    "url": "https://download.gnome.org/sources/gnome-maps/42/gnome-maps-42.3.tar.xz",
                    "sha256": "e456467facf3c8fd10c9386b5e74c43d9595ad9fb75257f7da014af983d69cd1",
                    "x-checker-data": {
