{
    "id": "org.gnome.Maps",
    "runtime": "org.gnome.Platform",
    "runtime-version": "44",
    "sdk": "org.gnome.Sdk",
    "command": "gnome-maps",
    "finish-args": [
        "--share=ipc",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--device=dri",
        "--share=network",
        "--metadata=X-DConf=migrate-path=/org/gnome/maps/"
    ],
    "cleanup": [
        "/include",
        "/lib/pkgconfig",
        "/share/pkgconfig",
        "/share/aclocal",
        "/man",
        "/share/man",
        "/share/gtk-doc",
        "/share/vala",
        "*.la",
        "*.a"
    ],
    "modules": [
        {
            "name": "geocode-glib",
            "buildsystem": "meson",
            "builddir": true,
            "config-opts": [
                "--libdir=/app/lib",
                "-Denable-gtk-doc=false",
                "-Dsoup2=false"
            ],
            "sources": [
                {
                    "type": "archive",
                    "url": "https://download.gnome.org/sources/geocode-glib/3.26/geocode-glib-3.26.4.tar.xz",
                    "sha256": "2d9a6826d158470449a173871221596da0f83ebdcff98b90c7049089056a37aa",
                    "x-checker-data": {
                        "type": "gnome",
                        "name": "geocode-glib"
                    }
                }
            ]
        },
        {
            "name": "libgweather",
            "config-opts": [
                "-Denable_vala=false",
                "-Dgtk_doc=false",
                "-Dsoup2=false"
            ],
            "buildsystem": "meson",
            "sources": [
                {
                    "type": "archive",
                    "url": "https://download.gnome.org/sources/libgweather/4.4/libgweather-4.4.0.tar.xz",
                    "sha256": "366e866ff2a708b894cfea9475b8e8ff54cb3e2b477ea72a8ade0dabee5f48a4",
                    "x-checker-data": {
                        "type": "gnome",
                        "name": "libgweather",
                        "versions": {
                            "<": "40.0",
                            ">=": "4.0.0"
                        }
                    }
                }
            ]
        },
        {
            "name": "librest",
            "buildsystem": "meson",
            "config-opts": [
                "-Dgtk_doc=false",
                "-Dexamples=false",
                "-Dsoup2=false"
            ],
            "sources": [
                {
                    "type": "archive",
                    "url": "https://download.gnome.org/sources/rest/0.9/rest-0.9.1.tar.xz",
                    "sha256": "9266a5c10ece383e193dfb7ffb07b509cc1f51521ab8dad76af96ed14212c2e3",
                    "x-checker-data": {
                        "type": "gnome",
                        "name": "rest",
                        "stable-only": false
                    }
                }
            ]
        },
        {
            "name": "libshumate",
            "buildsystem": "meson",
            "config-opts": [
                "-Ddemos=false",
                "-Dgtk_doc=false",
                "-Dvapi=false",
                "-Dlibsoup3=true"
            ],
            "sources": [
                {
                    "type": "archive",
                    "url": "https://download.gnome.org/sources/libshumate/1.0/libshumate-1.0.5.tar.xz",
                    "sha256": "a8cfd8df9177ee44b04733b017af13c9e93f98765cdc89b750a48edaf701956c",
                    "x-checker-data": {
                        "type": "gnome",
                        "stable-only": true,
                        "name": "libshumate"
                    }
                }
            ]
        },
        {
            "name": "gnome-maps",
            "buildsystem": "meson",
            "sources": [
                {
                    "type": "archive",
                    "url": "https://download.gnome.org/sources/gnome-maps/45/gnome-maps-45.0.tar.xz",
                    "sha256": "9b60107c3157162631b95bcba64c51af45e2d49009452863f84e1f2bcd46d610",
                    "x-checker-data": {
                        "type": "gnome",
                        "is-important": true,
                        "name": "gnome-maps"
                    }
                }
            ]
        }
    ]
}
