{
    "id": "org.gnome.Maps",
    "runtime": "org.gnome.Platform",
    "runtime-version": "44",
    "sdk": "org.gnome.Sdk",
    "command": "gnome-maps",
    "finish-args": [
        "--share=ipc",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--device=dri",
        "--share=network",
        "--talk-name=org.gnome.evolution.dataserver.AddressBook10",
        "--talk-name=org.gnome.evolution.dataserver.Calendar8",
        "--talk-name=org.gnome.evolution.dataserver.Sources5",
        "--talk-name=org.gnome.evolution.dataserver.Subprocess.Backend.*",
        "--talk-name=org.freedesktop.secrets",
        "--metadata=X-DConf=migrate-path=/org/gnome/maps/"
    ],
    "cleanup": [
        "/include",
        "/lib/pkgconfig",
        "/share/pkgconfig",
        "/share/aclocal",
        "/man",
        "/share/man",
        "/share/gtk-doc",
        "/share/vala",
        "*.la",
        "*.a"
    ],
    "modules": [
        {
            "name": "geocode-glib",
            "buildsystem": "meson",
            "builddir": true,
            "config-opts": [
                "--libdir=/app/lib",
                "-Denable-gtk-doc=false",
                "-Dsoup2=false"
            ],
            "sources": [
                {
                    "type": "archive",
                    "url": "https://download.gnome.org/sources/geocode-glib/3.26/geocode-glib-3.26.4.tar.xz",
                    "sha256": "2d9a6826d158470449a173871221596da0f83ebdcff98b90c7049089056a37aa",
                    "x-checker-data": {
                        "type": "gnome",
                        "name": "geocode-glib"
                    }
                }
            ]
        },
        {
            "name": "libgweather",
            "config-opts": [
                "-Denable_vala=false",
                "-Dgtk_doc=false",
                "-Dsoup2=false"
            ],
            "buildsystem": "meson",
            "sources": [
                {
                    "type": "archive",
                    "url": "https://download.gnome.org/sources/libgweather/4.2/libgweather-4.2.0.tar.xz",
                    "sha256": "af8a812da0d8976a000e1d62572c256086a817323fbf35b066dbfdd8d2ca6203",
                    "x-checker-data": {
                        "type": "gnome",
                        "name": "libgweather",
                        "versions": {
                            "<": "40.0",
                            ">=": "4.0.0"
                        }
                    }
                }
            ]
        },
        {
            "name": "librest",
            "buildsystem": "meson",
            "config-opts": [
                "-Dgtk_doc=false",
                "-Dexamples=false",
                "-Dsoup2=false"
            ],
            "sources": [
                {
                    "type": "archive",
                    "url": "https://gitlab.gnome.org/GNOME/librest/-/archive/0.9.1/librest-0.9.1.tar.gz",
                    "sha256": "d91247d7f8fd6c63e61bd27ad1a6ddb6819e5279286fc2ded84b5b8bf10b330e"
                }
            ]
        },
        {
            "name": "libshumate",
            "buildsystem": "meson",
            "config-opts": [
                "-Ddemos=false",
                "-Dgtk_doc=false",
                "-Dvapi=false",
                "-Dlibsoup3=true"
            ],
            "sources": [
                {
                    "type": "archive",
                    "url": "https://download.gnome.org/sources/libshumate/1.0/libshumate-1.0.3.tar.xz",
                    "sha256": "77a192f0dfec082b8de20b1939f3adf03eed41114d49fdaae2bbb2729ceb015a",
                    "x-checker-data": {
                        "type": "gnome",
                        "name": "libshumate"
                    }
                }
            ]
        },
        {
            "name": "gnome-maps",
            "buildsystem": "meson",
            "sources": [
                {
                    "type": "archive",
                    "url": "https://download.gnome.org/sources/gnome-maps/44/gnome-maps-44.0.tar.xz",
                    "sha256": "6003eb73dd9fd269b4a91bed9bf03ee9ab240c5124eedabf28be22a3fefba595",
                    "x-checker-data": {
                        "type": "gnome",
                        "name": "gnome-maps"
                    }
                }
            ]
        }
    ]
}
